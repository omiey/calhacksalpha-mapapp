package com.calhacksalpha.dawwwskigrowl.data;

import com.here.android.mpa.common.Image;
import com.here.android.mpa.mapping.MapMarker;

import android.R;

public class User {
	
	private MapMarker myMapMarker;
	private com.here.android.mpa.common.Image myImage;
	
	public User() {
		myImage = new Image();
		myImage.setImageResource(R.drawable.arrow_up_float);
		myMapMarker = new MapMarker(coordinate, myImage);
	}
}
