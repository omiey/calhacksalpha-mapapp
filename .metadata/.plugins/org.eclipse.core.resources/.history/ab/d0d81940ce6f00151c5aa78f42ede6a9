package com.calhacksalpha.dawwwskigrowl.data;

import java.util.List;

public class LocationEntities implements Drawable {
	private List<Drawable> entities;
	
	@Override
	public void drawLocation() {
		for (Drawable drawable : entities) {
			
			drawable.drawLocation();
			map.addMapObject(myMapMarker);
		}
	}
}
