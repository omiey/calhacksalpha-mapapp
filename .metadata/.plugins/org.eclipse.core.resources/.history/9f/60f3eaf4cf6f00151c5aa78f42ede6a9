public class LocationEntities implements LocationAware {

	private Map<User, LocationAware> entities = new HashMap<User, LocationAware>();

	public void addEntity(User d) {
		entities.put(d, d);
	}

	public void removeEntity(User d) {
		entities.remove(d);
	}

	@Override
	public void updateLocation(User u, GeoCoordinate coordinate) {

	}
}
